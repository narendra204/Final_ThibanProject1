    <style>
        .nav_menu {
            border-bottom: none;
        }

        .bg {
            background: #fff;
        }

        .menu_section {
            margin-bottom: 0px;
        }
        /*ul.nav.navbar-nav.navbar-right {
            width: 60%;
        }*/
        .nav_title {
            width: 233px;
        }

        input, select, textarea {
            border-radius: 3px !important;
        }

        ul.dropdown-menu.side-dropdown {
            position: absolute;
            top: 48px !important;
            left: 0px !important;
            border: 1px solid #eee !important;
            border-radius: 1px;
            background: #f2f2f2 !important;
            -webkit-box-shadow: none !important;
            box-shadow: none !important;
            background-clip: !important;
        }

            ul.dropdown-menu.side-dropdown a {
                color: #888 !important;
            }

        .dropdown:hover ul.dropdown-menu.side-dropdown {
            display: block;
            padding: 0px;
        }

        .dropdown.current-page.active .side-dropdown {
            display: none !important;
        }

        .dropdown.current-page.active:hover .side-dropdown {
            display: block !important;
        }

        .left_col {
            background: #f7f7f7;
            overflow-y: scroll;
            overflow-x: hidden;
            height: 100% !important;
            position: fixed;
        }

        .col-md-3.left_col {
            overflow: hidden;
        }

        .nav-sm #menu_toggle {
            margin-left: 10px;
        }

        .nav-sm .navbar-search {
            margin-left: 0px;
        }

        .side-menu li.current-page a {
            color: #57ad68 !important;
            background: #f2f2f2;
        }

        li.dropdown.current-page a:hover ul.dropdown-menu.side-dropdown {
            display: block !important;
        }

        .side-dropdown li a {
            background: #f7f7f7 !important;
            color: #888 !important;
        }

            .side-dropdown li a:hover {
                background: #f2f2f2 !important;
                color: #222;
            }

        ul.nav.side-menu.sub-menu li a {
            padding-left: 30px;
        }

        ul.nav.side-menu li.active a {
            background: transparent !important;
            color: #888;
        }

        ul.nav.side-menu.sub-menu li.current-page a {
            color: #57ad68 !important;
            background: #f2f2f2 !important;
        }

        a.dropdown-sub-menu {
            padding-left: 40px !important;
        }

        a.drop-down-mainmenu {
            cursor: default;
        }

        .dropdown-sub-menu img {
            width: 16px !important;
        }

        a.drop-down-mainmenu {
            color: #687888 !important;
            font-weight: 400 !important;
            font-family: FrutigerArabic-Bold,FrutigerArabic-Regular,Tahoma,Helvetica,sans-serif;
        }

            a.drop-down-mainmenu:hover {
                color: #687888 !important;
            }
    </style>
</head>

<body class="nav-md">
    <div class="container body main-container-body">
        <div class="main_container">
