@model Final_ThibanProject.Models.viewmodel.DiverRating
<script src="~/Scripts/jquery-3.1.1.min.js"></script>
<script type="text/javascript">
    $("#closbtn").click(function () {
        debugger;
        $('#myModal').hide();
        window.location.reload();
    });
    $("#btnclose").click(function () {
        debugger;
        $('#myModal').hide();
        window.location.reload();
    });
   
</script>
           
                    <div class="modal-header">
                        <button type="button" class="close" id="closbtn" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">DRIVER RATING &amp REVIEW</h4>
                    </div>
                    <div class="modal-body">
                        <div class="popup-content-block">
                            <h5>@Html.DisplayFor(model => model.name)</h5>
                            <p>
                                @{
                                    var base64 = Convert.ToBase64String(Model.image);
                                    var imgSrc = String.Format("data:image/gif;base64,{0}", base64);
                                }
                                <img src='@imgSrc' style="max-width:100px; max-height:100px;" />

                            </p>
                            <p><strong>Best Water is here</strong></p>
                            <p>@for(int i=1;i<=Model.rating; i++)
                               {<i class="fa fa-star"></i>}@Html.DisplayFor(model => model.rating)</p>
                            <p>@Html.DisplayFor(model => model.comment)</p>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" id="btnclose" data-dismiss="modal">Close</button>
                    </div>
               
     






